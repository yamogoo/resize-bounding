{
  "name": "vue3-resize-bounding",
  "description": "Simple Vue 3 component for resizing nested components",
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "yamogoo",
    "email": "grebennikovmisha@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:yamogoo/vue3-resize-bounding.git"
  },
  "type": "module",
  "keywords": [
    "vue3",
    "resize",
    "bounding box",
    "resize bounding",
    "resizable element"
  ],
  "main": "dist/vue3-resize-bounding.umd.js",
  "module": "dist/vue3-resize-bounding.es.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue3-resize-bounding.es.js",
      "types": "./dist/src/index.d.ts",
      "require": "./dist/vue3-resize-bounding.cjs.js"
    }
  },
  "files": [
    "dist/*"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json && vite build",
    "dev": "vite build --watch",
    "test:unit": "vitest",
    "test:ui": "vitest --ui"
  },
  "peerDependencies": {
    "vue": "^3.2"
  },
  "dependencies": {
    "deepmerge": "^4.3.1"
  },
  "devDependencies": {
    "@fluentui/merge-styles": "^8.6.12",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.8",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/ui": "^1.6.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.5.0",
    "jsdom": "^24.1.0",
    "sass": "^1.77.6",
    "terser": "^5.31.1",
    "typescript": "^5.2.2",
    "vite": "^5.3.2",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.24"
  }
}
