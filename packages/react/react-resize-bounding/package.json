{
  "name": "react-resize-bounding",
  "description": "React highly customizable component for resizing nested content",
  "version": "1.0.4",
  "license": "MIT",
  "private": false,
  "author": {
    "name": "yamogoo",
    "email": "grebennikovmisha@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:yamogoo/resize-bounding.git"
  },
  "type": "module",
  "keywords": [
    "react",
    "resizer",
    "react resizer",
    "resize",
    "bounding box",
    "resize bounding",
    "resize container",
    "draggable container"
  ],
  "main": "dist/react-resize-bounding.umd.js",
  "module": "dist/react-resize-bounding.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-resize-bounding.es.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/react-resize-bounding.cjs.js"
    }
  },
  "files": [
    "dist/*",
    "lib/*"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json && vite build",
    "dev": "vite build --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test:unit": "vitest",
    "test:ui": "vitest --ui"
  },
  "peerDependencies": {
    "react": "16.8.0 || 17.x",
    "react-dom": "16.8.0 || 17.x"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.3.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "jsdom": "^24.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^2.0.2"
  },
  "dependencies": {
    "@fluentui/merge-styles": "^8.6.12",
    "deepmerge": "^4.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
