{
  "name": "vue3-resize-bounding",
  "description": "Vue3 highly customizable component for resizing nested content",
  "version": "2.0.3",
  "license": "MIT",
  "private": false,
  "author": {
    "name": "yamogoo",
    "email": "grebennikovmisha@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:yamogoo/resize-bounding.git"
  },
  "type": "module",
  "keywords": [
    "vue3",
    "resizer",
    "vue3 resizer",
    "resize",
    "bounding box",
    "resize bounding",
    "resize container",
    "draggable container"
  ],
  "main": "dist/vue3-resize-bounding.umd.js",
  "module": "dist/vue3-resize-bounding.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue3-resize-bounding.es.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/vue3-resize-bounding.cjs.js"
    }
  },
  "files": [
    "dist/*",
    "lib/*"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json && vite build",
    "dev": "vite build --watch",
    "test:unit": "vitest",
    "test:ui": "vitest --ui"
  },
  "peerDependencies": {
    "vue": "^3.2"
  },
  "devDependencies": {
    "@fluentui/merge-styles": "^8.6.12",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.10",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/ui": "^2.0.2",
    "@vue/test-utils": "^2.4.6",
    "deepmerge": "^4.3.1",
    "eslint": "^9.5.0",
    "jsdom": "^24.1.0",
    "sass": "^1.77.8",
    "terser": "^5.31.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^2.0.2",
    "vue-tsc": "^2.0.24"
  }
}
